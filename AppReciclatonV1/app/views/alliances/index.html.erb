<p id="notice"><%= notice %></p>

<h1>Listing Alliances</h1>

<table>
  <thead>
    <tr>
      <th>Client supplier</th>
      <th>Client recipient</th>
      <th>Supply</th>
      <th>Demand</th>
      <th>Agreed quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alliances.each do |alliance| %>
      <tr>
        <td><%= alliance.client_supplier_id %></td>
        <td><%= alliance.client_recipient_id %></td>
        <td><%= alliance.supply_id %></td>
        <td><%= alliance.demand_id %></td>
        <td><%= alliance.agreed_quantity %></td>
        <td><%= link_to 'Show', alliance %></td>
        <td><%= link_to 'Edit', edit_alliance_path(alliance) %></td>
        <td><%= link_to 'Destroy', alliance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alliance', new_alliance_path %>
